@{
    ViewData["Title"] = "Pending";
}

<h1 id="teamwelcome" class="text-center">
    Welcome <span id="teamwelcometeam">@Contest.Team?.TeamName</span>!
</h1>
<h2 id="contestnotstarted" class="text-center">
    Contest has not been started
</h2>

<p class="text-center mt-4 pt-3" asp-show-if="Contest.Team == null && Contest.Settings.CanRegister()">
    <a asp-action="Register" class="btn btn-primary"><i class="fas fa-rocket"></i> Register as participant</a>
</p>
